function bmiCalculator (weight, height) {
    var interpretation=weight/(height*height);
if(interpretation<18.5) 
{
    console.log( "your bmi is "+ interpretation + " you are underweight");
}else if(interpretation>=18.5 && interpretation<=24.9){
    console.log(  "your bmi is "+ interpretation + " you are normal weight");
}else(interpretation>24.9)
{
    console.log(  "your bmi is "+ interpretation + " you are overweight");
}
    return interpretation;
}
bmiCalculator(54,1.57);
